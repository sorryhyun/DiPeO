{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/DBNodeData",
  "definitions": {
    "DBNodeData": {
      "additionalProperties": false,
      "properties": {
        "collection": {
          "type": "string"
        },
        "data": {
          "$ref": "#/definitions/Record<string,any>"
        },
        "file": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "string"
            }
          ]
        },
        "flipped": {
          "type": "boolean"
        },
        "format": {
          "type": "string"
        },
        "glob": {
          "type": "boolean"
        },
        "label": {
          "type": "string"
        },
        "operation": {
          "type": "string"
        },
        "query": {
          "type": "string"
        },
        "serialize_json": {
          "type": "boolean"
        },
        "sub_type": {
          "$ref": "#/definitions/DBBlockSubType"
        }
      },
      "required": [
        "label",
        "operation",
        "sub_type"
      ],
      "type": "object"
    },
    "Record<string,any>": {
      "additionalProperties": false,
      "type": "object"
    },
    "DBBlockSubType": {
      "description": "Node-specific enumerations",
      "enum": [
        "fixed_prompt",
        "file",
        "code",
        "api_tool"
      ],
      "type": "string"
    }
  }
}