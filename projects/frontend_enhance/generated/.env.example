# API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000/ws

# Feature Flags
VITE_ENABLE_OFFLINE=true
VITE_ENABLE_ANALYTICS=false
VITE_FEATURE_REAL_TIME_UPDATES=true
VITE_FEATURE_DATA_VISUALIZATION=true
VITE_FEATURE_ADVANCED_FILTERING=true
VITE_FEATURE_EXPORT=true

# Application Settings
VITE_LOG_LEVEL=info
VITE_DEFAULT_THEME=system

# Pagination
VITE_DEFAULT_PAGE_SIZE=20
VITE_MAX_PAGE_SIZE=100

# Cache Settings (in milliseconds)
VITE_CACHE_STALE_TIME=300000
VITE_CACHE_TIME=600000

# WebSocket Settings (in milliseconds)
VITE_WS_RECONNECT_INTERVAL=5000
VITE_WS_MAX_RECONNECT_ATTEMPTS=5