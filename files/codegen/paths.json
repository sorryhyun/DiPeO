{
  "description": "Centralized path configuration for DiPeO codegen system",
  "version": "1.0.0",
  
  "base_paths": {
    "project_root": ".",
    "codegen_root": "files/codegen",
    "dipeo_core": "dipeo",
    "frontend_root": "apps/web",
    "backend_root": "apps/server"
  },
  
  "source_paths": {
    "typescript_models": "dipeo/models/src",
    "node_specifications": "files/codegen/specifications/nodes",
    "templates": "files/codegen/templates",
    "schemas": "files/codegen/schemas",
    "code": "files/codegen/code",
    "manifests": "files/codegen/manifests"
  },
  
  "output_paths": {
    "models": {
      "typescript": "dipeo/models/src/nodes/{{spec_data.nodeType}}Node.ts",
      "python_generated": "dipeo/models",
      "python_static": "dipeo/core/static/nodes"
    },
    "graphql": {
      "schema": "apps/server/src/dipeo_server/api/graphql/schema/nodes/{{spec_data.nodeType}}.graphql"
    },
    "frontend": {
      "components": "apps/web/src/features/diagram-editor/components/nodes/generated/{{spec_data.nodeType}}Node.tsx",
      "node_configs": "apps/web/src/features/diagram-editor/config/nodes/generated/{{spec_data.camelCase}}Config.ts",
      "field_configs": "apps/web/src/features/diagram-editor/config/nodes/generated/{{spec_data.camelCase}}Fields.ts",
      "generated_types": "apps/web/src/__generated__"
    },
    "registries": {
      "node_types": "dipeo/core/registries/node_types.py",
      "frontend_nodes": "apps/web/src/features/diagram-editor/config/nodeRegistry.ts"
    }
  },
  
  "template_files": {
    "typescript_model": "files/codegen/templates/typescript_model.hbs",
    "python_model": "files/codegen/templates/pydantic_models.j2",
    "graphql_schema": "files/codegen/templates/graphql_schema.hbs",
    "react_component": "files/codegen/templates/react_component.hbs",
    "node_config": "files/codegen/templates/node_config.hbs",
    "field_config": "files/codegen/templates/field_config.hbs",
    "conversions": "files/codegen/templates/codegen/conversions.j2",
    "zod_schemas": "files/codegen/templates/codegen/zod_schemas.j2"
  },
  
  "schema_files": {
    "node_specification": "files/codegen/schemas/node_specification.schema.json"
  },
  
  "code_files": {
    "node_spec_loader": "files/codegen/code/node_spec_loader.py",
    "codegen_functions": "files/codegen/code/codegen_functions.py",
    "domain_model_functions": "files/codegen/code/domain_model_functions.py",
    "type_transformers": "files/codegen/code/type_transformers.py",
    "json_schema_generator": "files/codegen/code/json_schema_generator.py",
    "registry_functions": "files/codegen/code/registry_functions.py",
    "sub_diagram_helpers": "files/codegen/code/phase3/sub_diagram_helpers.py"
  },
  
  "generated_file_patterns": {
    "typescript_model": "{{spec_data.nodeType}}Node.ts",
    "python_model": "__generated_models__.py",
    "python_conversions": "__generated_conversions__.py",
    "graphql_schema": "{{spec_data.nodeType}}.graphql",
    "react_component": "{{spec_data.nodeType}}Node.tsx",
    "node_config": "{{spec_data.camelCase}}Config.ts",
    "field_config": "{{spec_data.camelCase}}Fields.ts"
  },
  
  "file_extensions": {
    "typescript": [".ts", ".tsx"],
    "python": [".py"],
    "graphql": [".graphql", ".gql"],
    "json": [".json"],
    "yaml": [".yaml", ".yml"]
  }
}