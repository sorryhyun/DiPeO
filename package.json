{
  "name": "dipeo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "DiPeO - Diagrammed People & Organizations: AI-powered agent workflows through visual programming",
  "scripts": {
    "dev": "pnpm --filter web dev",
    "dev:web": "pnpm --filter web dev",
    "build": "pnpm -r build",
    "build:web": "pnpm --filter web build",
    "build:packages": "pnpm --filter '@dipeo/*' build",
    "preview:web": "pnpm --filter web preview",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "typecheck": "pnpm -r typecheck",
    "test": "pnpm -r test",
    "clean": "pnpm -r clean",
    "codegen": "pnpm --filter web codegen",
    "build:windows": "powershell -ExecutionPolicy Bypass -File ./scripts/build-all.ps1"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.15.29",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^16.2.0",
    "ts-morph": "^26.0.0",
    "typescript-eslint": "^8.32.1"
  },
  "packageManager": "pnpm@10.11.1+sha512.e519b9f7639869dc8d5c3c5dfef73b3f091094b0a006d7317353c72b124e80e1afd429732e28705ad6bfa1ee879c1fce46c128ccebd3192101f43dd67c667912"
}
